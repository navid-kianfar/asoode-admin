{
  "name": "asoode-admin",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build_asoode": "ng build --prod --aot -c=asoode --outputPath ../deploy-asoode/build/admin",
    "build_alrahah": "ng build --prod --aot -c=alrahah --outputPath ../deploy-alrahah/build/admin",
    "build_ciblsoft": "ng build --prod --aot -c=ciblsoft --outputPath ../deploy-ciblsoft/build/admin",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "npm run format_ts && npm run format_html && npm run format_scss",
    "format_ts": "prettier --write \"src/**/*.ts\"",
    "format_html": "prettier --write \"src/**/*.html\"",
    "format_scss": "prettier --write \"src/**/*.scss\""
  },
  "private": true,
  "dependencies": {
    "@angular-slider/ngx-slider": "^2.0.3",
    "@angular/animations": "~11.0.5",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@angular/service-worker": "~11.0.5",
    "apexcharts": "^3.24.0",
    "device-uuid": "^1.0.4",
    "jalali-moment": "^3.3.9",
    "moment-hijri": "^2.1.2",
    "ng-apexcharts": "^1.5.7",
    "ngx-ckeditor": "^10.0.0",
    "ngx-cookie-service": "^11.0.2",
    "ngx-device-detector": "^2.0.4",
    "ngx-google-analytics": "^11.0.0",
    "ngx-simple-modal": "^1.4.14",
    "ngx-socket-io": "^3.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
